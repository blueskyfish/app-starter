{
  "name": "@blueskyfish/app-starter",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "docker:build": "docker compose -f ./docker/docker-compose.dev.yml build --no-cache",
    "docker:serve": "docker compose -p app-starter -f ./docker/docker-compose.dev.yml up -d --quiet-pull",
    "docker:stop": "docker compose -p app-starter -f ./docker/docker-compose.dev.yml down",
    "postinstall": "ngcc --properties es2020 browser module main",
    "generate:user": "ng-openapi-gen --config packages/frontend/lib/user-api/user-api.backend.json",
    "i18n:extract": "transloco-keys-manager extract -i './packages/frontend/' -o './packages/frontend/app/shell-app/src'",
    "i18n:find": "transloco-keys-manager find -i './packages/frontend' -o './packages/frontend/app/shell-app/src'"
  },
  "author": {
    "name": "Blueskyfish",
