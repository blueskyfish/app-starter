{
  "projectType": "library",
  "sourceRoot": "packages/frontend/lib/user/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/packages/frontend/lib/user"],
      "options": {
        "project": "packages/frontend/lib/user/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/frontend/lib/user/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/frontend/lib/user/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/frontend/lib/user"],
      "options": {
        "jestConfig": "packages/frontend/lib/user/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "packages/frontend/lib/user/**/*.ts",
          "packages/frontend/lib/user/**/*.html"
        ]
      }
    }
  },
  "tags": ["frontend:lib"]
}
